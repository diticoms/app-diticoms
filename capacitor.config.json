{
  "appId": "vn.diticoms.service",
  "appName": "Diticoms Service",
  "webDir": "dist",
  "server": {
    "url": "https://service.diticoms.vn",
    "allowNavigation": [
      "service.diticoms.vn",
      "*.google.com",
      "*.googleusercontent.com",
      "*.vietqr.io"
    ]
  },
  "android": {
    "buildOptions": {
      "releaseType": "APK"
    },
    "allowMixedContent": true,
    "captureInput": true
  },
  "ios": {
    "contentInset": "always"
  }
}